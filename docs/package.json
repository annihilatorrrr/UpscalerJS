{
  "name": "@upscalerjs/docs",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "copy-examples": "rimraf ./docs/usage/**/*.md && ts-node scripts/copy-examples.ts",
    "copy-examples:watch": "watch \"ts-node scripts/copy-examples.ts\" ../examples",
    "build-api-docs": "ts-node scripts/build-api-docs.ts",
    "docusaurus": "docusaurus",
    "start": "TYPEDOC_WATCH=true pnpm copy-examples && concurrently \"docusaurus start\" \"pnpm copy-examples:watch\"",
    "build": "pnpm copy-examples && docusaurus build",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.1",
    "@docusaurus/preset-classic": "2.0.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-github-btn": "^1.3.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.1",
    "@docusaurus/types": "^2.0.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "typescript": "^4.7.4",
    "@docusaurus/core": "2.0.1",
    "@docusaurus/preset-classic": "2.0.1",
    "docusaurus": "2.0.1",
    "docusaurus-plugin-typedoc": "^0.17.5",
    "typedoc": "^0.22.13",
    "typedoc-plugin-markdown": "*",
    "watch": "^1.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
